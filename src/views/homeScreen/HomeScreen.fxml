            <Label layoutX="415.0" layoutY="146.0" text="Farming World" textAlignment="CENTER">
               <font>
                  <Font name="Britannic Bold" size="69.0" />
               </font>
            </Label>
            <ImageView fitHeight="92.0" fitWidth="60.0" layoutX="21.0" layoutY="645.0" pickOnBounds="true" preserveRatio="true">
