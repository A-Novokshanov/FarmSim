<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox prefHeight="720.0" prefWidth="1280.0" style="-fx-border-color: Gray;" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.homeScreenView.HomeScreenViewController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Label layoutX="415.0" layoutY="146.0" text="Farming World" textAlignment="CENTER">
                    <font>
                        <Font name="Britannic Bold" size="69.0"/>
                    </font>
                </Label>
                <ImageView fitHeight="92.0" fitWidth="60.0" layoutX="21.0" layoutY="645.0" pickOnBounds="true"
                           preserveRatio="true">
                    <image>
                        <Image url="@../../dependencies/images/unnamed.png"/>
                    </image>
                </ImageView>
                <JFXButton fx:id="btnContinue" layoutX="1120.0" layoutY="523.0" onMouseClicked="#continueGame"
                           style="-fx-border-color: Gray; -jfx-disable-visual-focus: True;" text="Continue">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </JFXButton>
                <JFXButton fx:id="btnNewGame" layoutX="1104.0" layoutY="593.0" onMouseClicked="#newGame"
                           style="-fx-border-color: Gray;" text="New Game">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </JFXButton>
            </children>
        </AnchorPane>
    </children>
</VBox>